<Project>

	<PropertyGroup>
		<TargetFrameworks>net6.0;net9.0</TargetFrameworks>
		<LangVersion>preview</LangVersion>
		<EnablePreviewFeatures>True</EnablePreviewFeatures>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<DebugType>embedded</DebugType>
		<Version>0.1.0</Version>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<Authors>vlad2048</Authors>
		<Description>Composable reactive variables</Description>
		<PackageTags>Reactive,Rx,C#,Variable</PackageTags>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<PackageProjectUrl>https://github.com/vlad2048/PowRxVar</PackageProjectUrl>
		<SolutionDirectory>$(MSBuildThisFileDirectory.TrimEnd('\\'))</SolutionDirectory>
	</PropertyGroup>

	<ItemGroup>
		<None Include="$(SolutionDirectory)\README.md" Pack="true" PackagePath="\" />
	</ItemGroup>

	<PropertyGroup>
		<NoWarn>CS1591</NoWarn>
	</PropertyGroup>

</Project>